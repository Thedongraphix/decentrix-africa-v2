---
import type { ImageSource } from "@utils/types";

const { src, alt } = Astro.props;

interface Props {
  src: ImageSource;
  alt: string;
}
---

<img
  class="inline-block h-8 w-8 rounded-full ring-2 ring-neutral-50 dark:ring-zinc-800"
  src={typeof src === 'string' ? src : src.src}
  alt={alt}
  loading="eager"
/>
